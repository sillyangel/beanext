{
    "manifest_version": 3,
    "name": "Beans",
    "version": "1.0.0",
    "description": "Beans",
    "action": {
      "default_popup": "popup.html",
      "default_title": "Beans"
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "16": "icon16.png",
      "32": "icon32.png",
      "128": "icon128.png"
    }
  }
  